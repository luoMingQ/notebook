<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lsc.notebook.dao.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lsc.notebook.entity.Menu">
        <id column="menu_id" property="menuId" />
        <result column="patient_menu_id" property="patientMenuId" />
        <result column="menu_name" property="menuName" />
        <result column="menu_url" property="menuUrl" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        menu_id, patient_menu_id, menu_name, menu_url
    </sql>

</mapper>
